{
  "log": {
    "loglevel": "info"
  },
  "inbounds": [
    {
      "port": 1080,
      "listen": "127.0.0.1",
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls", "quic"]
      }
    }
  ],
  "outbounds": [
    {
      "tag": "proxy-chain-entry",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "entry-server.example.com",
            "port": 443,
            "users": [
              {
                "id": "ffc677c5-0ac8-4ce1-8413-c2daf5a7fedb",
                "encryption": "none",
                "flow": "xtls-rprx-vision"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "serverName": "entry-server.example.com",
          "allowInsecure": false,
          "fingerprint": "chrome"
        }
      },
      "proxySettings": {
        "tag": "proxy-chain-middle",
        "transportLayer": true
      }
    },
    {
      "tag": "proxy-chain-middle",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "middle-server.example.com",
            "port": 443,
            "users": [
              {
                "id": "900a60b3-2f42-444f-9193-bca56d2390de",
                "alterId": 0,
                "security": "auto"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "wsSettings": {
          "path": "/vmess-ws"
        },
        "tlsSettings": {
          "serverName": "middle-server.example.com",
          "allowInsecure": false
        }
      },
      "proxySettings": {
        "tag": "proxy-chain-exit",
        "transportLayer": true
      }
    },
    {
      "tag": "proxy-chain-exit",
      "protocol": "socks",
      "settings": {
        "servers": [
          {
            "address": "exit-server.example.com",
            "port": 1080,
            "users": [
              {
                "user": "username",
                "pass": "password"
              }
            ]
          }
        ]
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "block",
      "protocol": "blackhole",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "domain": [
          "geosite:category-ads-all"
        ],
        "outboundTag": "block"
      },
      {
        "type": "field",
        "ip": [
          "geoip:private",
          "geoip:cn"
        ],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "domain": [
          "geosite:cn"
        ],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "network": "tcp,udp",
        "outboundTag": "proxy-chain-entry"
      }
    ]
  }
}
